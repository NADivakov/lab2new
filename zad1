#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

void main(void)
 {
	setlocale(LC_ALL,"Russian");
	int x,y;
	float a,b,c;
	printf("enter x:");
	scanf("%d",&x);
	printf("enter y:");
	scanf("%d",&y);
	a=pow(x,2);
	b=pow(y,2);
	c=pow(a+b,1/2);
	if (a=1) printf("yes");
		else printf("no");
	}
